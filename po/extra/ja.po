#: data/bluetooth.appdata.xml.in:8
msgid "Bluetooth Indicator"
msgstr "Bluetooth インジケーター"

#: data/bluetooth.appdata.xml.in:9
msgid "Connect and disconnect paired Bluetooth devices"
msgstr "ペアリングした Bluetooth デバイスを接続したり切断したりします"

#: data/bluetooth.appdata.xml.in:14
msgid "Minor updates:"
msgstr "軽微なアップデート:"

#: data/bluetooth.appdata.xml.in:16
msgid "Improve restoring Bluetooth state on restart and resuming from suspend"
msgstr "再起動やサスペンドからの復帰時に、適切な Bluetooth の状態を復元するように修正"

#: data/bluetooth.appdata.xml.in:17 data/bluetooth.appdata.xml.in:25
#: data/bluetooth.appdata.xml.in:33 data/bluetooth.appdata.xml.in:38
#: data/bluetooth.appdata.xml.in:43
msgid "Updated translations"
msgstr "翻訳の更新"

#: data/bluetooth.appdata.xml.in:23
msgid "Add tooltip on hover"
msgstr "ホバー時のツールチップを追加"

#: data/bluetooth.appdata.xml.in:24
msgid "Add a fallback device name for headphones"
msgstr ""
"ヘッドフォンの製品名が検出できない場合に“ヘッドフォン”と表示するように修正"

#: data/bluetooth.appdata.xml.in:30
msgid "Prevent showing duplicate devices"
msgstr "デバイスが重複して表示される不具合を修正"

#: data/bluetooth.appdata.xml.in:31
msgid "Fix animated icon when disabling Bluetooth"
msgstr "Bluetooth を無効にするときに表示されるアニメーションアイコンを修正"

#: data/bluetooth.appdata.xml.in:32
msgid "Use device type as fallback instead of \"(null)\""
msgstr ""
"デバイス名が利用可能ではない場合に、\"(null)\" と表示するのではなくデバイスの"
"種類を表示するように修正"

#: data/bluetooth.appdata.xml.in:82
msgid "elementary, Inc."
msgstr "elementary, Inc."
