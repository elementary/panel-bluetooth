<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/io/elementary/desktop/wingpanel/bluetooth/" id="io.elementary.desktop.wingpanel.bluetooth">
		<key type="b" name="bluetooth-enabled">
			<default>true</default>
			<summary>Sets the last bluetooth state to re-apply on reboot</summary>
			<description></description>
		</key>
		<key type="b" name="bluetooth-obex-enabled">
			<default>true</default>
			<summary>Sets registered agent obex</summary>
			<description></description>
		</key>
		<key type="i" name="bluetooth-accept-files">
			<default>0</default>
			<summary>Sets accept mode</summary>
			<description></description>
		</key>
	</schema>
</schemalist>
